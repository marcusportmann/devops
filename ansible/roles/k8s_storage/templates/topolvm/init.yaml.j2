apiVersion: v1
kind: Namespace
metadata:
  name: topolvm-system
  labels:
    app.kubernetes.io/name: topolvm-system
    topolvm.cybozu.com/webhook: ignore
---
apiVersion: v1
kind: Secret
metadata:
  name: mutatingwebhook
  namespace: topolvm-system
data:
  tls.key: {{ topolvm_mutatingwebhook_key }}
  tls.crt: {{ topolvm_mutatingwebhook_crt }}
type: kubernetes.io/tls

